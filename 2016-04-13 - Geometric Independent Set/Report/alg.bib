	doi = {10.1007/s00493-011-2403-3},
	abstract = {Given independent random points \$X\_1,...,X\_n{\textbackslash}in{\textbackslash}eR{\textasciicircum}d\$ with common probability distribution \${\textbackslash}nu\$, and a positive distance \$r=r(n){\textgreater}0\$, we construct a random geometric graph \$G\_n\$ with vertex set \${\textbackslash}\{1,...,n{\textbackslash}\}\$ where distinct \$i\$ and \$j\$ are adjacent when \${\textbackslash}norm\{X\_i-X\_j\}{\textbackslash}leq r\$. Here \${\textbackslash}norm\{.\}\$ may be any norm on \${\textbackslash}eR{\textasciicircum}d\$, and \${\textbackslash}nu\$ may be any probability distribution on \${\textbackslash}eR{\textasciicircum}d\$ with a bounded density function. We consider the chromatic number \${\textbackslash}chi(G\_n)\$ of \$G\_n\$ and its relation to the clique number \${\textbackslash}omega(G\_n)\$ as \$n {\textbackslash}to {\textbackslash}infty\$. Both McDiarmid and Penrose considered the range of \$r\$ when \$r {\textbackslash}ll ({\textbackslash}frac\{{\textbackslash}ln n\}\{n\}){\textasciicircum}\{1/d\}\$ and the range when \$r {\textbackslash}gg ({\textbackslash}frac\{{\textbackslash}ln n\}\{n\}){\textasciicircum}\{1/d\}\$, and their results showed a dramatic difference between these two cases. Here we sharpen and extend the earlier results, and in particular we consider the `phase change' range when \$r {\textbackslash}sim ({\textbackslash}frac\{t{\textbackslash}ln n\}\{n\}){\textasciicircum}\{1/d\}\$ with \$t{\textgreater}0\$ a fixed constant. Both McDiarmid and Penrose asked for the behaviour of the chromatic number in this range. We determine constants \$c(t)\$ such that \${\textbackslash}frac\{{\textbackslash}chi(G\_n)\}\{nr{\textasciicircum}d\}{\textbackslash}to c(t)\$ almost surely. Further, we find a "sharp threshold" (except for less interesting choices of the norm when the unit ball tiles \$d\$-space): there is a constant \$t\_0{\textgreater}0\$ such that if \$t {\textbackslash}leq t\_0\$ then \${\textbackslash}frac\{{\textbackslash}chi(G\_n)\}\{{\textbackslash}omega(G\_n)\}\$ tends to 1 almost surely, but if \$t {\textgreater} t\_0\$ then \${\textbackslash}frac\{{\textbackslash}chi(G\_n)\}\{{\textbackslash}omega(G\_n)\}\$ tends to a limit \${\textgreater}1\$ almost surely.},
	number = {4},
	urldate = {2016-03-19},
	journal = {Combinatorica},
	author = {McDiarmid, Colin and Müller, Tobias},
	month = nov,
	year = {2011},
	note = {arXiv: 1101.6065},
	keywords = {Mathematics - Combinatorics, Mathematics - Probability},
	pages = {423--488},
	annote = {Many informations on chromatic number (X) which is related to the size of the biggest independet set (S)
XS {\textgreater}= {\textbar}V{\textbar}+1
 },
}

@article{agarwal,
	title = {Independent set of intersection graphs of convex objects in 2D},
	volume = {34},
	issn = {0925-7721},
	url = {http://www.sciencedirect.com/science/article/pii/S0925772106000095},
	doi = {10.1016/j.comgeo.2005.12.001},
	abstract = {The intersection graph of a set of geometric objects is defined as a graph G = ( S , E ) in which there is an edge between two nodes s i , s j ∈ S if s i ∩ s j ≠ ∅ . The problem of computing a maximum independent set in the intersection graph of a set of objects is known to be NP-complete for most cases in two and higher dimensions. We present approximation algorithms for computing a maximum independent set of intersection graphs of convex objects in R 2 . Specifically, given (i) a set of n line segments in the plane with maximum independent set of size α, we present algorithms that find an independent set of size at least ( α / ( 2 log ( 2 n / α ) ) ) 1 / 2 in time O ( n 3 ) and ( α / ( 2 log ( 2 n / α ) ) ) 1 / 4 in time O ( n 4 / 3 log c n ) , (ii) a set of n convex objects with maximum independent set of size α, we present an algorithm that finds an independent set of size at least ( α / ( 2 log ( 2 n / α ) ) ) 1 / 3 in time O ( n 3 + τ ( S ) ) , assuming that S can be preprocessed in time τ ( S ) to answer certain primitive operations on these convex sets, and (iii) a set of n rectangles with maximum independent set of size βn, for β ⩽ 1 , we present an algorithm that computes an independent set of size Ω ( β 2 n ) . All our algorithms use the notion of partial orders that exploit the geometric structure of the convex objects.},
	number = {2},
	urldate = {2016-03-19},
	journal = {Computational Geometry},
	author = {Agarwal, Pankaj K. and Mustafa, Nabil H.},
	month = may,
	year = {2006},
	keywords = {Approximation algorithms, Convex objects, Geometric intersection graphs, Maximum independent set},
	pages = {83--95},
	annote = {Independet set of convex objects},
}



