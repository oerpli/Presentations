	keywords = {Algorithm Analysis and Problem Complexity, Data Structures, Discrete Mathematics in Computer Science, Numeric Computing, Simulation and Modeling, Theory of Computation},
	pages = {214--221},
	annote = {Some algorithm that yields an approximate solution},
}

@article{mcdiarmid,
	title = {On the chromatic number of random geometric graphs},
